{
  "name": "student-tracker-app",
  "version": "1.0.0",
  "main": "src/electron/main.ts",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "concurrently \"next dev --turbopack\" \"wait-on http://localhost:3000 && electron -r ts-node/register ./src/electron/main.ts\"",
    "build": "next build --turbopack",
    "start": "next start",
    "electron:build": "tsc -p src/electron && electron-builder",
    "dev:electron": "electron -r ts-node/register src/electron/main.ts"
  },
  "_moduleAliases": {
    "@": "src"
  },
  "dependencies": {
    "@fullcalendar/core": "^6.1.19",
    "@fullcalendar/daygrid": "^6.1.19",
    "@fullcalendar/interaction": "^6.1.19",
    "@fullcalendar/react": "^6.1.19",
    "@fullcalendar/timegrid": "^6.1.19",
    "bcryptjs": "^3.0.2",
    "chart.js": "^4.5.0",
    "flatpickr": "^4.6.13",
    "mongoose": "^8.18.0",
    "next": "15.5.2",
    "next-auth": "^4.24.11",
    "next-themes": "^0.4.6",
    "nodemailer": "^6.10.1",
    "react": "19.1.0",
    "react-big-calendar": "^1.19.4",
    "react-chartjs-2": "^5.3.0",
    "react-dom": "19.1.0",
    "react-flatpickr": "^4.0.11",
    "react-modal": "^3.16.3"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/nodemailer": "^7.0.1",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "concurrently": "^9.2.1",
    "electron": "^38.2.0",
    "electron-builder": "^26.0.12",
    "eslint": "^9",
    "eslint-config-next": "15.5.2",
    "tailwindcss": "^4",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3",
    "wait-on": "^9.0.1"
  }
}
